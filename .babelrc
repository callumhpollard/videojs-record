{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 3 versions"]
      },
      "debug": false,
      "modules": "commonjs"
    }]
  ],
  "plugins": ["add-module-exports"],
  "env": {
      "test": {
        "plugins": [
          ["istanbul", {
            "exclude": [
              "test/**/*.spec.js"
          ]
        }]
      ]
    }
  }
}
